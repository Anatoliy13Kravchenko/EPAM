{
  "name": "epam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf yarn.lock && yarn",
    "dev": "yarn development",
    "development": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "yarn development --watch",
    "test": "node_modules/.bin/jest",
    "server": "yarn watch --server",
    "server-nomap": "yarn watch --server --nomap",
    "code": "tslint --project tsconfig.json --exclude 'types/**/*.d.ts' --config tslint.json 'resources/_src/**/*.{ts,tsx}'",
    "analyze": "yarn development --analyze",
    "analyze-prod": "yarn production --analyze",
    "gulp-html": "gulp html",
    "gulp-sass": "gulp sass"
  },
